def input_to_index("1" - "9")
  puts '1' .to_i
  puts '2' .to_i
  puts '3' .to_i
  puts '4' .to_i
  puts '5' .to_i
  puts '6' .to_i
  puts '7' .to_i
  puts '8' .to_i
  puts '9' .to_i
end

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
def update_array_at_with(array, index, value)
  array[index] = value
end 
